{
  "expo": {
    "name": "해우소",
    "slug": "haewuso",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/haewuso_logo.png",
    "splash": {
      "image": "./assets/images/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff",
      "enableFullScreenImage_legacy": true
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.minwoojung.haewuso",
      "icon": "./assets/images/haewuso_logo.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "앱 사용 중 위치 권한을 요청합니다.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "백그라운드 위치 접근이 필요합니다.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "icon": "./assets/images/haewuso_logo_aos.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/haewuso_logo_aos.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.minwoojung.haewuso"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "977728ba-cb7e-413b-8ebd-a127bda9578b"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "android": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash1.png",
            "resizeMode": "cover",
            "imageWidth": 200
          },
          "ios": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/splash1.png",
            "resizeMode": "cover",
            "imageWidth": 200
          }
        }
      ]
    ]
  }
}
