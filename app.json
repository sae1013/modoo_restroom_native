{
  "expo": {
    "name": "해우소",
    "slug": "haewuso",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/haewuso_logo.png",
    "splash": "./assets/images/splash1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.minwoojung.haewuso",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "앱 사용 중 위치 권한을 요청합니다.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "백그라운드 위치 접근이 필요합니다."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/haewuso_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.minwoojung.haewuso"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash1.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "977728ba-cb7e-413b-8ebd-a127bda9578b"
      }
    }
  }
}
